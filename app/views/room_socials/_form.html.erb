<div class="fg-container">

  <%= simple_form_for [@room, @room_social] do |f| %>
    <div class="fg-form">
      <%= f.input :social_type, collection: RoomSocial::SOCIAL_TYPES.each_slice(1).to_a, required: true %>
      <%= f.input :url, placeholder:"eg: https://www.instagram.com/pitstop.live/" %>
      <%= f.button :submit, class: "cta-btn" %>
    </div>
  <% end %>

</div>